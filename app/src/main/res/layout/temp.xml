<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/renj6pri9icj"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@drawable/cr50b005982">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/rb5wyisdlrgk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginTop="37dp"
                android:layout_marginBottom="40dp"
                android:layout_marginHorizontal="24dp"
                android:orientation="horizontal">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rpx80hu9008"
                    app:shapeAppearanceOverlay="@style/cr50"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/r7734vpnje2f"
                    app:shapeAppearanceOverlay="@style/cr50"
                    android:layout_width="52dp"
                    android:layout_height="52dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rsrao6f8sp2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="@drawable/sffffffsw2cr10"
                android:paddingVertical="13dp"
                android:paddingHorizontal="20dp"
                android:layout_marginBottom="34dp"
                android:layout_marginHorizontal="24dp"
                android:orientation="horizontal">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rag8c0abuu9r"
                    app:shapeAppearanceOverlay="@style/cr50"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginEnd="10dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
                <EditText
                    android:inputType="text"
                    android:hint="Search"
                    android:background="@null"
                    android:id="@+id/r9z9uwosisml"
                    android:textColor="#FFFFFF"
                    android:textSize="18sp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rd82hghblmze"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:paddingTop="41dp"
                android:paddingBottom="29dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/ri0ad4x7smjj"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:layout_marginHorizontal="24dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/r67bgtiyqngo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/cr10bbcbcbc"
                        android:paddingVertical="9dp"
                        android:layout_marginEnd="10dp"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/rofwxmjo3r5"
                            android:textColor="#000000"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:layout_marginHorizontal="37dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Sports"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/rtmgwzkw2ym"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/s7979794dsw1cr10"
                        android:paddingVertical="9dp"
                        android:layout_marginEnd="10dp"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/rfo7a15qxoh"
                            android:textColor="#797979"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:layout_marginHorizontal="28dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Academic"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/rcjyu6o4jy5b"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/s7979794dsw1cr10"
                        android:paddingVertical="9dp"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/rgzgmjbosw7g"
                            android:textColor="#797979"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:layout_marginHorizontal="10dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Faculty Events"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/r4k44qgbjmw"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cr15bffffff"
                    android:paddingVertical="27dp"
                    android:layout_marginBottom="18dp"
                    android:layout_marginHorizontal="24dp"
                    android:elevation="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/newsTitle"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        android:layout_marginBottom="6dp"
                        android:layout_marginHorizontal="23dp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="Faculty of Technology Clinches Victory in the Inter-Faculty Cricket Tournament 2025"
                        />
                    <TextView
                        android:id="@+id/newsDate"
                        android:textColor="#000000"
                        android:textSize="11sp"
                        android:layout_marginBottom="180dp"
                        android:layout_marginStart="23dp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="April 28, 2025"
                        />
                    <TextView
                        android:id="@+id/rguclq0ub57s"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:layout_marginHorizontal="20dp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="The Faculty of Technology secured a thrilling win in the Inter-Faculty Cricket Cup 2025, defeating the Faculty of Management in the final overs. ."
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rb4q1oegxoz"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cr15bffffff"
                    android:paddingVertical="14dp"
                    android:layout_marginHorizontal="24dp"
                    android:elevation="15dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/r7hdek5sd99k"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        android:layout_marginBottom="6dp"
                        android:layout_marginStart="26dp"
                        android:layout_height="wrap_content"
                        android:layout_width="227dp"
                        android:text="Faculty of Technology Clinches Victory in the Inter-Faculty Cricket Tournament 2025"
                        />
                    <TextView
                        android:id="@+id/rirw667oyhkf"
                        android:textColor="#000000"
                        android:textSize="11sp"
                        android:layout_marginBottom="4dp"
                        android:layout_marginStart="27dp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="April 28, 2025"
                        />
                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/rmryu6m02i9"
                        app:shapeAppearanceOverlay="@style/cr50"
                        android:layout_width="match_parent"
                        android:layout_height="141dp"
                        android:layout_marginBottom="13dp"
                        android:layout_marginHorizontal="20dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/r3kmgnsog0hn"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        android:layout_marginHorizontal="20dp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="FoT athletes broke records at the UoC Annual Sports Meet with standout performances."
                        />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
